<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <googlerecaptcha>
            <backend>
                <enabled>0</enabled>
                <size>normal</size>
                <theme>light</theme>
            </backend>
            <frontend>
                <enabled>0</enabled>
                <type>invisible</type>
                <forms>body.customer-account-create #form-validate.form-create-account</forms>
                <position>bottomleft</position>
                <theme>light</theme>
            </frontend>
        </googlerecaptcha>
    </default>
</config>
